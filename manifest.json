{
	"manifest_version": 2,
	"name": "GeoBetter",
	"version": "1.0",
	"description": "Shows solo ranks and stats everywhere on GeoGuessr",
	"icons": {
		"48": "icons/icon.png",
		"96": "icons/icon.png"
	},
	"permissions": ["https://vid.burger.supply/*", "https://geoguessr.com/*"],
	"content_scripts": [
		{
			"matches": ["*://*.geoguessr.com/*"],
			"js": ["content/script.js"],
			"run_at": "document_idle"
		}
	]
}
